{
  "name": "react-vanilla-switch",
  "displayName": "React-Vanilla Switch",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:react-switch.switchToVanilla",
    "onCommand:react-switch.switchToReact"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-switch.switchToVanilla",
        "title": "Switch to Vanilla"
      },
      {
        "command": "react-switch.switchToReact",
        "title": "Switch to React"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
